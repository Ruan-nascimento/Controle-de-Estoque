generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


model Item {
  id String @id @default(uuid())
  name String
  type String
  qtd Int @default(0)
  value Float @default(0)
  flavor String
  status String @default("Em Falta")
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt @default(now())
}